{
  "expo": {
    "name": "Tour in Nepal",
    "slug": "AwesomeProject",
    "privacy": "public",
    "sdkVersion": "33.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "android": {
      "package": "com.jeevanaayush.tourinnepal",
      "permissions": [
         "ACCESS_FINE_LOCATION"
      ]
    },
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.jeevanaayush.tourinnepal",
      "supportsTablet": true
    },
    "description": "copyright Jeevan Gyawali"
  }
}